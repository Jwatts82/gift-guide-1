<br>
<h2>My Gift Lists</h2>
<hr>
    <div>
        <form  action="/gifts">
            <input  name="q" type="text" placeholder="Find a Gift">
            <button type="submit">Search</button>
        </form>
    </div>
<br>
<% @lists.each do |list| %>
<ul>
<h4><%= link_to list.name, list_path(list) %></h4>
</ul>
<% end %>
<br>
<br>
<button><%=  link_to "Create a Gift List", new_gift_path %></button>


